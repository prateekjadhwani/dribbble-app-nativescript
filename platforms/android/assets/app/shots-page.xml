<Page loaded="pageLoaded">
	<Label text="Shots!!" cssClass="message"/>
	<ListView id="myListView" items="{{ myItems }}" itemTap="listViewItemTap" >
		<ListView.itemTemplate>
			<StackLayout orientation="vertical">
				<Image src="{{ images.hidpi ? images.hidpi : images.normal }}" stretch="aspectFill" />
				<Label text="{{ title }}" cssClass="message"/>
			</StackLayout>
		</ListView.itemTemplate>
	</ListView>
</Page>